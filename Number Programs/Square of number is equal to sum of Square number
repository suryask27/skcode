import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int t=n;
        int sq=t*t;
        int sum=0;
        while(sq>0)
        {
            int r=sq%10;
            sum=sum+r;
            sq=sq/10;
        }
        if(n==sum)//Square of number is equal to sum of Square number
        System.out.print("Yes");
        else
        System.out.print("No");
        }
}
