import java.util.*;
public class Main
{
public static void main(String[] args){
   Scanner s=new Scanner(System.in);  
   int n=s.nextInt();
   int count=0;
   for(int i=1;i<n/2;i++)
   {
       if((i*i)==(n+1))
       count++;
   }
       if(count>0)
          System.out.println("Yes");
       else
          System.out.println("No");
 }
}

