import java.util.*;
public class Main
{
public static void main(String[] args){
   Scanner s=new Scanner(System.in);  
   int n=s.nextInt();
   int m=n;
   int rem;
   int sum=0;
   while(n!=0)
   {
       rem=n%10;
        int f=1;
        for(int i=1;i<=rem;i++)
        f=f*i;
        sum=sum+f;
       n/=10;
       
   }
   if(sum==m)
    System.out.println("Yes");
   else
    System.out.println("No");
 }
}
