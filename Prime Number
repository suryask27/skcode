import java.util.*;
public class Main
{
public static void main(String[] args){
   Scanner s=new Scanner(System.in);  
   int n=4,m=0;
    m=n/2;
   int count=0;
   if(n==0 ||n==1 )
       System.out.println("No");
    else{
   for(int i=2;i<=m;i++){
    if(n%i==0){
        System.out.println("No");
        count=1;
        break;
    }
    }
   if(count==0){
    System.out.println("Yes");
   }
    }
 }
}
