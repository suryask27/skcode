import java.util.*;
public class Main
{
public static void main(String[] args){
   Scanner s=new Scanner(System.in);  
   int n=s.nextInt();
   int m=n;
   int rem;
   int sum=0;
   n=n*n;
   while(n!=0)
   {
       rem=n%10;
       sum=sum+rem;
       n/=10;
   }
   if(sum==m)
    System.out.println("Yes");
   else
    System.out.println("No");
 }
}
