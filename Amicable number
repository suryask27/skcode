import java.util.*;
public class Main
{
public static void main(String[] args){
   Scanner s=new Scanner(System.in);  
   int n=s.nextInt();
   int m=s.nextInt();
   int sum=0;
   int sum1=0;
   for(int i=1;i<=n/2;i++)
	   if(n%i==0)
	       sum=sum+i;
   for(int i=1;i<=m/2;i++)
	   if(m%i==0)
	       sum1=sum1+i;
   if(sum==m && sum1==n)
       System.out.println("Yes");
   else
       System.out.println("No");
 }
}
